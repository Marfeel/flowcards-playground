{
	"cards": {
		"video": {
			"content": {
				"type": "AMPDocument",
				"url": "${PLAYGROUND_PROXY}/${CURRENT_HOSTNAME}/experiences/video/video.html"
			},
			"snapPoints": {
				"initial": {
					"interactive": true,
					"value": 0.615
				},
				"minimised": {
					"interactive": true,
					"value": 0.82
				},
				"active": {
					"interactive": true,
					"value": 80
				}
			},
			"features": {
				"button": {
					"action": "setSnapPoint:initial"
				},
				"mode": "modal",
				"removable": true
			},
			"triggers": {
				"myScrollTrigger": {
					"on": "scroll",
					"action": "setSnapPoint:initial",
					"spec": {
						"type": "absolute",
						"pixel": 300,
						"percentage": 0.1
					}
				}
			}
		}
	}
}
