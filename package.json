{
  "name": "flowcards-experiences",
  "version": "0.0.0-development",
  "description": "Flowcards Experiences",
  "files": [
    "conf/**/*",
    "experiences/**/*.test.js",
    "tests/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Marfeel/flowcards-playground.git"
  },
  "scripts": {
    "local": "./node_modules/.bin/wdio conf/local.conf.js",
    "single": "./node_modules/.bin/wdio conf/single.conf.js",
    "single:local": "./node_modules/.bin/wdio conf/single.local.conf.js",
    "single:local:dev": "./node_modules/.bin/wdio conf/single.local.server.conf.js",
    "parallel": "./node_modules/.bin/wdio conf/parallel.conf.js",
    "appium:tab": "ttab -q -G -t 'Appium Server' npm run appium",
    "appium": "appium"
  },
  "publishConfig": {
    "registry": "https://repositories.mrf.io/nexus/repository/npm-internal/"
  },
  "author": "Marfeel",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Marfeel/flowcards-playground/issues"
  },
  "homepage": "https://github.com/Marfeel/flowcards-playground#readme",
  "devDependencies": {
    "@wdio/local-runner": "^7.3.1",
    "@wdio/mocha-framework": "^7.3.1",
    "@wdio/static-server-service": "^7.3.1",
    "@wdio/sync": "^7.3.1",
    "appium": "^1.20.2",
    "appium-doctor": "^1.16.0",
    "chai": "^4.3.4",
    "semantic-release": "^17.4.2"
  },
  "dependencies": {
    "@wdio/cli": "^7.3.1",
    "browserstack-local": "^1.4.8",
    "dotenv": "^8.2.0"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github",
      "@semantic-release/npm"
    ]
  }
}
